<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 第一层 -->
    <item android:left="6dp" android:right="6dp">
        <shape>
            <solid android:color="#09000000" />
            <corners android:radius="40dip" />
        </shape>
    </item>
    <!-- 第二层 -->
    <item
        android:bottom="5dp"
        android:left="6dp"
        android:right="6dp">
